pipeline{
    agent any
    environment {
        CI = 'true'
    }

    stages{
        stage ('Install dependencies react project'){
            steps{
                echo 'Start install dependencies react project'
                sh 'npm install'
            }
        }
        stage('Test project'){
            steps{
                echo "testing here"
            }
        }
        stage('Build project'){
            steps{
                sh 'npm run build'
            }
        }
    }
}